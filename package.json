{
  "name": "@inksafari/engawa",
  "description": "Personal blog for @inksafari. Built in Svelte using kit(Vite).",
  "version": "0.0.0-development",
  "private": true,
  "license-template": "MIT",
  "license-posts": "CC BY-NC 4.0",
  "license-images": "All images are copyright to their respective owners.",
  "author": "Han-Yu Lin[@inksafari] (https://inksafari.net)",
  "engines": {
    "node": ">= 14.17.*"
  },
  "scripts": {
    "prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "dev": "run-s clean dev:kit",
    "dev:css": "cross-env NODE_ENV=development postcss src/global.pcss -o static/global.css -w",
    "dev:kit": "svelte-kit dev --port 9000 --open",
    "chk": "svelte-check",
    "build:": "run-s clean build:site",
    "build:css": "cross-env NODE_ENV=production postcss src/global.pcss -o static/global.css",
    "build:site": "svelte-kit build",
    "build:preview": "svelte-kit preview --port 9000 --open",
    "clean": "rm -rf .svelte-kit build .vercelignore .vercel_build_output",
    "nuke": "run-s clean dev:kit",
    "serve": "run-s clean build:site build:preview",
    "lint:js": "pnpx eslint --ignore-path .gitignore \"{,!(node_modules|public|build|.svelte-kit)/**/}*.{js,ts,svelte}\" ",
    "fix:js": "npm run lint:js --fix",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "a11y": "svelte-kit build && a11y 'build/**/*.html'",
    "load:css": "sh ./scripts/critical-css.sh",
    "load:glyphs": "cd ./build && font-spider --no-backup --map \"/static\",\"./static\" *.html pages/**/*.html posts/**/*.html"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/vite-plugin-svelte": "next",
    "@tsconfig/svelte": "^1.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.11.23",
    "esbuild-svelte": "^0.5.3",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "font-spider": "^1.3.5",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "mdsvex": "^0.9.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-load-config": "^3.1.0",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-preset-minify": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "remark-abbr": "^1.4.1",
    "rollup": "^2.53.1",
    "sass": "^1.35.2",
    "svelte": "^3.38.3",
    "svelte-check": "^2.2.2",
    "svelte-preprocess": "^4.7.4",
    "svelte-preprocess-esbuild": "^2.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "vite-imagetools": "^3.6.8"
  },
  "resolutions": {},
  "volta": {
    "node": "14.17.0"
  },
  "browserslist": [
    "> 5% in alt-AS",
    "last 2 major versions and supports es6-module",
    "not IE <=11",
    "maintained node versions",
    "not dead"
  ],
  "type": "module",
  "typings": "src/global.d.ts"
}